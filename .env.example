# Puerto del servidor
PORT=3000

# URL del banco emisor (issuing bank)
ISSUER_BASE_URL=http://localhost:8080

# Entorno de ejecución
NODE_ENV=development

# Nivel de logs (debug, info, warn, error)
LOG_LEVEL=info

# Habilitar logging a archivo (en desarrollo por defecto está deshabilitado)
ENABLE_FILE_LOGGING=false
